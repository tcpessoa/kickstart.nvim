  },
  "Console Log Object with Color JSON.stringify": {
    "prefix": "clogo",
    "body": [
      "console.log(`\\x1b[1;30;42m ${1:someVar}: \\${JSON.stringify(${1}, null, 2)} \\x1b[0m`);"
    ],
